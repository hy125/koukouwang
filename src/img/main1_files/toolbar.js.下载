!function(o){function n(){var n=o(window).height();e.height(n),t.height(n),i.height(n),j.height(n),_.height(n)}function a(o){o?(t.removeClass("open"),r=!1):(t.addClass("open"),r=!0)}var e=o("#J_toolbar"),t=o("#J_toolbar").find(".jj_toolbar"),i=o("#J_toolbar").find(".jj_toolbar_content"),j=o("#J_toolbar").find(".jj_toolbar_content_wrap"),_=o("#J_toolbar").find(".jj_toolbar_tabs"),d=o("#J_toolbar").find(".jj_toolbar_header"),l=o("#J_toolbar").find(".jj_toolbar_tab_server"),f=o("#jj_close"),r=!1;n(),o(window).resize(function(){n()}),d.click(function(){a(r)}),f.click(function(){console.log(1),r=!0,a(r)}),o(".jj_backTop").click(function(){o("html,body").stop().animate({scrollTop:0})}),l.mouseover(function(){l.find(".jj_expand").show(),l.find(".jj_expand").removeClass("jj_ani_fadeOut"),l.find(".jj_expand").addClass("jj_ani_fadeIn")}).mouseleave(function(){l.find(".jj_expand").removeClass("jj_ani_fadeIn"),l.find(".jj_expand").addClass("jj_ani_fadeOut"),setTimeout(function(){l.find(".jj_expand").hide()},200)})}(jQuery);